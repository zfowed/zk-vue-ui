  "repository": {
    "type": "git",
    "url": "git+https://github.com/zfowed/zk-vue-ui.git"
  },
  "dependencies": {
    "@zfowed/utils": "^1.0.22",
    "async-validator": "^1.11.2",
    "clipboard": "^2.0.4",
    "core-js": "^2.6.5",
    "element-ui": "^2.8.2",
    "glob": "^7.1.4",
    "html2canvas": "^1.0.0-rc.1",
