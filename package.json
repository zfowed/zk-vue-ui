{
  "name": "zk-vue-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 9000",
    "build": "npm run build:docs && npm run build:dist",
    "build:docs": "vue-cli-service build --target lib --name zk-vue-ui packages/index.js --dest docs/public/libs/zk-vue-ui",
    "build:dist": "vue-cli-service build --target lib --name zk-vue-ui packages/index.js --dest dist",
    "pub": "npm run build && npm publish",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@zfowed/utils": "^1.0.22",
    "async-validator": "^1.11.2",
    "axios": "^0.18.0",
    "clipboard": "^2.0.4",
    "core-js": "^2.6.5",
    "element-ui": "^2.8.2",
    "popper.js": "^1.15.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
